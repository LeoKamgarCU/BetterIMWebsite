    <form action="/register" method="POST">
        <div class="input-group mb-3">
            <input type="text" class="form-control" name= "username" id = "username" aria-describedby="userHelp"
                    placeholder="Username" required>
        </div>
  
          









        <div class="input-group mb-3">
            <input type="password" class="form-control" name= "password" id = "password" aria-describedby="userHelp"
            placeholder="Password" required>
        </div>

        <div class="input-group mb-3">
            <input type="password" class="form-control" name= "password" id = "confirmPassword" aria-describedby="userHelp"
            placeholder="Confirm Password" required>
        </div>
        <div class="input-group mb-3">
          <label class="input-group-text" for="inputGroupSelect01">Class of</label>
            <select class="form-select" id="inputGroupSelect01" required>
                <option selected disabled value=""></option>
                <option value="2023">2023</option>
                <option value="2024">2024</option>
                <option value="2025">2025</option>
                <option value="2026">2026</option>
                <option value="NA">Other/NA</option>
            </select>
        </div>


        <div class="input-group mb-3">
          <label class="input-group-text" for="inputGroupFile01">Profile Photo</label>
          <input type="file" accept=".jpg,.gif,.png" class="form-control" id="inputGroupFile01">
        </div>
        </form>
        